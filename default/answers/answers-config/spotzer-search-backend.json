{
  "$schema": "https://schema.yext.com/config/answers/answers-config/v1",
  "$id": "spotzer-search-backend",
  "name": "Spotzer Search Backend",
  "supportedLocales": [],
  "countryRestrictions": [],
  "verticals": {
    "faqs": {
      "entityTypes": [
        "faq"
      ],
      "name": "FAQs",
      "searchableFields": {
        "answerV2": {
          "nlpFilter": true,
          "textSearch": true
        },
        "builtin.entityType": {
          "nlpFilter": true
        },
        "keywords": {
          "textSearch": true
        },
        "question": {
          "nlpFilter": true,
          "textSearch": true
        }
      },
      "sortBys": [],
      "source": "YEXT"
    },
    "jobs": {
      "entityTypes": [
        "job"
      ],
      "name": "Jobs",
      "searchableFields": {
        "builtin.entityType": {
          "nlpFilter": true,
          "textSearch": true
        },
        "builtin.location": {
          "nlpFilter": true
        },
        "employmentType": {
          "nlpFilter": true,
          "textSearch": true
        },
        "keywords": {
          "phraseMatch": true
        },
        "name": {
          "textSearch": true
        }
      },
      "sortBys": [],
      "source": "YEXT"
    },
    "products": {
      "entityTypes": [
        "product"
      ],
      "name": "Products",
      "searchableFields": {
        "builtin.entityType": {
          "nlpFilter": true
        },
        "name": {
          "semanticTextSearch": true
        }
      },
      "sortBys": [],
      "source": "YEXT"
    }
  },
  "localizations": {},
  "additionalSearchablePlaces": [],
  "querySuggestions": {
    "disablePopularQueries": false,
    "verticalPromptsOnEmpty": false,
    "popularQueriesBlacklistedTerms": [],
    "popularQueriesBlacklistedRegex": [],
    "universalPrompts": [],
    "verticalPrompts": {
      "faqs": [
        "[[name]]"
      ],
      "jobs": [
        "[[name]]"
      ],
      "products": [
        "[[name]]"
      ]
    }
  }
}